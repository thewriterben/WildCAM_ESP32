/**
 * @file secrets.h.example
 * @brief Template for WiFi credentials configuration
 * 
 * INSTRUCTIONS:
 * 1. Copy this file to "secrets.h" in the same directory
 * 2. Replace the placeholder values with your actual WiFi credentials
 * 3. DO NOT commit secrets.h to version control
 * 
 * The secrets.h file is included by config.h and should contain:
 * - WIFI_SSID: Your WiFi network name
 * - WIFI_PASSWORD: Your WiFi network password
 */

#ifndef SECRETS_H
#define SECRETS_H

/**
 * @brief WiFi SSID (network name)
 * @note Replace with your actual WiFi network name
 * @example "MyHomeNetwork"
 */
#define WIFI_SSID "YourSSID"

/**
 * @brief WiFi password
 * @note Replace with your actual WiFi network password
 * @example "MySecurePassword123"
 */
#define WIFI_PASSWORD "YourPassword"

#endif // SECRETS_H
