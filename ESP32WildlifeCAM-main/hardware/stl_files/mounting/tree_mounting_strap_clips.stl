solid tree_mounting_strap_clips
  comment Clips for securing camera to trees - ESP32-S3-CAM compatible
  comment Dimensions: 50mm x 30mm x 15mm per clip (2 clips total)
  comment Features: Compatible with 25-50mm tree straps, anti-slip design
  comment Material: PETG or ASA for weather resistance
  comment Print settings: 0.2mm layer height, 50% infill for strength

  // Clip 1 - Left mounting clip
  // Main body (50x30x15mm)
  // Bottom face
  facet normal 0.0 0.0 -1.0
    outer loop
      vertex 0.0 0.0 0.0
      vertex 50.0 0.0 0.0
      vertex 50.0 30.0 0.0
    endloop
  endfacet
  facet normal 0.0 0.0 -1.0
    outer loop
      vertex 0.0 0.0 0.0
      vertex 50.0 30.0 0.0
      vertex 0.0 30.0 0.0
    endloop
  endfacet

  // Top face
  facet normal 0.0 0.0 1.0
    outer loop
      vertex 0.0 0.0 15.0
      vertex 50.0 30.0 15.0
      vertex 50.0 0.0 15.0
    endloop
  endfacet
  facet normal 0.0 0.0 1.0
    outer loop
      vertex 0.0 0.0 15.0
      vertex 0.0 30.0 15.0
      vertex 50.0 30.0 15.0
    endloop
  endfacet

  // Strap channel (25mm wide channel through center)
  // Front channel opening
  facet normal 0.0 -1.0 0.0
    outer loop
      vertex 12.5 2.5 0.0
      vertex 12.5 2.5 15.0
      vertex 37.5 2.5 15.0
    endloop
  endfacet
  facet normal 0.0 -1.0 0.0
    outer loop
      vertex 12.5 2.5 0.0
      vertex 37.5 2.5 15.0
      vertex 37.5 2.5 0.0
    endloop
  endfacet

  // Back channel opening
  facet normal 0.0 1.0 0.0
    outer loop
      vertex 12.5 27.5 0.0
      vertex 37.5 27.5 15.0
      vertex 12.5 27.5 15.0
    endloop
  endfacet
  facet normal 0.0 1.0 0.0
    outer loop
      vertex 12.5 27.5 0.0
      vertex 37.5 27.5 0.0
      vertex 37.5 27.5 15.0
    endloop
  endfacet

  // Channel bottom
  facet normal 0.0 0.0 -1.0
    outer loop
      vertex 12.5 2.5 5.0
      vertex 37.5 2.5 5.0
      vertex 37.5 27.5 5.0
    endloop
  endfacet
  facet normal 0.0 0.0 -1.0
    outer loop
      vertex 12.5 2.5 5.0
      vertex 37.5 27.5 5.0
      vertex 12.5 27.5 5.0
    endloop
  endfacet

  // Channel sides
  facet normal -1.0 0.0 0.0
    outer loop
      vertex 12.5 2.5 5.0
      vertex 12.5 27.5 15.0
      vertex 12.5 2.5 15.0
    endloop
  endfacet
  facet normal -1.0 0.0 0.0
    outer loop
      vertex 12.5 2.5 5.0
      vertex 12.5 27.5 5.0
      vertex 12.5 27.5 15.0
    endloop
  endfacet

  facet normal 1.0 0.0 0.0
    outer loop
      vertex 37.5 2.5 5.0
      vertex 37.5 27.5 15.0
      vertex 37.5 27.5 5.0
    endloop
  endfacet
  facet normal 1.0 0.0 0.0
    outer loop
      vertex 37.5 2.5 5.0
      vertex 37.5 2.5 15.0
      vertex 37.5 27.5 15.0
    endloop
  endfacet

  // Outer walls (left side)
  // Front wall
  facet normal 0.0 -1.0 0.0
    outer loop
      vertex 0.0 0.0 0.0
      vertex 0.0 0.0 15.0
      vertex 12.5 0.0 15.0
    endloop
  endfacet
  facet normal 0.0 -1.0 0.0
    outer loop
      vertex 0.0 0.0 0.0
      vertex 12.5 0.0 15.0
      vertex 12.5 0.0 0.0
    endloop
  endfacet

  facet normal 0.0 -1.0 0.0
    outer loop
      vertex 37.5 0.0 0.0
      vertex 37.5 0.0 15.0
      vertex 50.0 0.0 15.0
    endloop
  endfacet
  facet normal 0.0 -1.0 0.0
    outer loop
      vertex 37.5 0.0 0.0
      vertex 50.0 0.0 15.0
      vertex 50.0 0.0 0.0
    endloop
  endfacet

  // Back wall
  facet normal 0.0 1.0 0.0
    outer loop
      vertex 0.0 30.0 0.0
      vertex 12.5 30.0 15.0
      vertex 0.0 30.0 15.0
    endloop
  endfacet
  facet normal 0.0 1.0 0.0
    outer loop
      vertex 0.0 30.0 0.0
      vertex 12.5 30.0 0.0
      vertex 12.5 30.0 15.0
    endloop
  endfacet

  facet normal 0.0 1.0 0.0
    outer loop
      vertex 37.5 30.0 0.0
      vertex 50.0 30.0 15.0
      vertex 37.5 30.0 15.0
    endloop
  endfacet
  facet normal 0.0 1.0 0.0
    outer loop
      vertex 37.5 30.0 0.0
      vertex 50.0 30.0 0.0
      vertex 50.0 30.0 15.0
    endloop
  endfacet

  // Left wall
  facet normal -1.0 0.0 0.0
    outer loop
      vertex 0.0 0.0 0.0
      vertex 0.0 30.0 15.0
      vertex 0.0 0.0 15.0
    endloop
  endfacet
  facet normal -1.0 0.0 0.0
    outer loop
      vertex 0.0 0.0 0.0
      vertex 0.0 30.0 0.0
      vertex 0.0 30.0 15.0
    endloop
  endfacet

  // Right wall
  facet normal 1.0 0.0 0.0
    outer loop
      vertex 50.0 0.0 0.0
      vertex 50.0 30.0 15.0
      vertex 50.0 30.0 0.0
    endloop
  endfacet
  facet normal 1.0 0.0 0.0
    outer loop
      vertex 50.0 0.0 0.0
      vertex 50.0 0.0 15.0
      vertex 50.0 30.0 15.0
    endloop
  endfacet

  // Anti-slip teeth (inside channel)
  // Tooth 1 (left side)
  facet normal 1.0 0.0 0.0
    outer loop
      vertex 15.0 10.0 5.0
      vertex 15.0 12.0 7.0
      vertex 15.0 10.0 7.0
    endloop
  endfacet
  facet normal 1.0 0.0 0.0
    outer loop
      vertex 15.0 10.0 5.0
      vertex 15.0 12.0 5.0
      vertex 15.0 12.0 7.0
    endloop
  endfacet

  facet normal 0.707 0.0 0.707
    outer loop
      vertex 15.0 10.0 7.0
      vertex 16.0 12.0 8.0
      vertex 15.0 12.0 7.0
    endloop
  endfacet
  facet normal 0.707 0.0 0.707
    outer loop
      vertex 15.0 10.0 7.0
      vertex 16.0 10.0 8.0
      vertex 16.0 12.0 8.0
    endloop
  endfacet

  // Tooth 2 (left side)
  facet normal 1.0 0.0 0.0
    outer loop
      vertex 15.0 18.0 5.0
      vertex 15.0 20.0 7.0
      vertex 15.0 18.0 7.0
    endloop
  endfacet
  facet normal 1.0 0.0 0.0
    outer loop
      vertex 15.0 18.0 5.0
      vertex 15.0 20.0 5.0
      vertex 15.0 20.0 7.0
    endloop
  endfacet

  facet normal 0.707 0.0 0.707
    outer loop
      vertex 15.0 18.0 7.0
      vertex 16.0 20.0 8.0
      vertex 15.0 20.0 7.0
    endloop
  endfacet
  facet normal 0.707 0.0 0.707
    outer loop
      vertex 15.0 18.0 7.0
      vertex 16.0 18.0 8.0
      vertex 16.0 20.0 8.0
    endloop
  endfacet

  // Tooth 3 (right side)
  facet normal -1.0 0.0 0.0
    outer loop
      vertex 35.0 10.0 5.0
      vertex 35.0 12.0 7.0
      vertex 35.0 12.0 5.0
    endloop
  endfacet
  facet normal -1.0 0.0 0.0
    outer loop
      vertex 35.0 10.0 5.0
      vertex 35.0 10.0 7.0
      vertex 35.0 12.0 7.0
    endloop
  endfacet

  facet normal -0.707 0.0 0.707
    outer loop
      vertex 35.0 10.0 7.0
      vertex 34.0 12.0 8.0
      vertex 35.0 12.0 7.0
    endloop
  endfacet
  facet normal -0.707 0.0 0.707
    outer loop
      vertex 35.0 10.0 7.0
      vertex 34.0 10.0 8.0
      vertex 34.0 12.0 8.0
    endloop
  endfacet

  // Tooth 4 (right side)
  facet normal -1.0 0.0 0.0
    outer loop
      vertex 35.0 18.0 5.0
      vertex 35.0 20.0 7.0
      vertex 35.0 20.0 5.0
    endloop
  endfacet
  facet normal -1.0 0.0 0.0
    outer loop
      vertex 35.0 18.0 5.0
      vertex 35.0 18.0 7.0
      vertex 35.0 20.0 7.0
    endloop
  endfacet

  facet normal -0.707 0.0 0.707
    outer loop
      vertex 35.0 18.0 7.0
      vertex 34.0 20.0 8.0
      vertex 35.0 20.0 7.0
    endloop
  endfacet
  facet normal -0.707 0.0 0.707
    outer loop
      vertex 35.0 18.0 7.0
      vertex 34.0 18.0 8.0
      vertex 34.0 20.0 8.0
    endloop
  endfacet

  // Camera mounting boss (25x15x5mm on top)
  facet normal 0.0 0.0 1.0
    outer loop
      vertex 12.5 7.5 15.0
      vertex 37.5 22.5 20.0
      vertex 12.5 22.5 20.0
    endloop
  endfacet
  facet normal 0.0 0.0 1.0
    outer loop
      vertex 12.5 7.5 15.0
      vertex 37.5 7.5 15.0
      vertex 37.5 22.5 20.0
    endloop
  endfacet

  // Boss sides
  facet normal 0.0 -1.0 0.0
    outer loop
      vertex 12.5 7.5 15.0
      vertex 12.5 7.5 20.0
      vertex 37.5 7.5 20.0
    endloop
  endfacet
  facet normal 0.0 -1.0 0.0
    outer loop
      vertex 12.5 7.5 15.0
      vertex 37.5 7.5 20.0
      vertex 37.5 7.5 15.0
    endloop
  endfacet

  facet normal 0.0 1.0 0.0
    outer loop
      vertex 12.5 22.5 15.0
      vertex 37.5 22.5 20.0
      vertex 12.5 22.5 20.0
    endloop
  endfacet
  facet normal 0.0 1.0 0.0
    outer loop
      vertex 12.5 22.5 15.0
      vertex 37.5 22.5 15.0
      vertex 37.5 22.5 20.0
    endloop
  endfacet

  facet normal -1.0 0.0 0.0
    outer loop
      vertex 12.5 7.5 15.0
      vertex 12.5 22.5 20.0
      vertex 12.5 7.5 20.0
    endloop
  endfacet
  facet normal -1.0 0.0 0.0
    outer loop
      vertex 12.5 7.5 15.0
      vertex 12.5 22.5 15.0
      vertex 12.5 22.5 20.0
    endloop
  endfacet

  facet normal 1.0 0.0 0.0
    outer loop
      vertex 37.5 7.5 15.0
      vertex 37.5 22.5 20.0
      vertex 37.5 22.5 15.0
    endloop
  endfacet
  facet normal 1.0 0.0 0.0
    outer loop
      vertex 37.5 7.5 15.0
      vertex 37.5 7.5 20.0
      vertex 37.5 22.5 20.0
    endloop
  endfacet

  // Mounting holes in boss (2x 6mm holes)
  // Hole 1
  facet normal 0.0 0.0 -1.0
    outer loop
      vertex 20.0 12.0 17.5
      vertex 23.0 12.0 17.5
      vertex 23.0 15.0 17.5
    endloop
  endfacet
  facet normal 0.0 0.0 -1.0
    outer loop
      vertex 20.0 12.0 17.5
      vertex 23.0 15.0 17.5
      vertex 20.0 15.0 17.5
    endloop
  endfacet

  // Hole 2
  facet normal 0.0 0.0 -1.0
    outer loop
      vertex 27.0 15.0 17.5
      vertex 30.0 18.0 17.5
      vertex 27.0 18.0 17.5
    endloop
  endfacet
  facet normal 0.0 0.0 -1.0
    outer loop
      vertex 27.0 15.0 17.5
      vertex 30.0 15.0 17.5
      vertex 30.0 18.0 17.5
    endloop
  endfacet

endsolid tree_mounting_strap_clips