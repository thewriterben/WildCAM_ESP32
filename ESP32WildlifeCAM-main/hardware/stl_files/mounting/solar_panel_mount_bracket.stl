      vertex 100.0 85.0 5.0
    endloop
  endfacet
  facet normal 1.0 0.0 0.0
    outer loop
      vertex 100.0 75.0 5.0
      vertex 100.0 75.0 15.0
      vertex 100.0 85.0 15.0
    endloop
  endfacet

  // Camera mount connection points (front of bracket)
  // Mount point 1 (left)
  facet normal 0.0 0.0 1.0
    outer loop
      vertex 10.0 -10.0 5.0
      vertex 20.0 0.0 10.0
      vertex 10.0 0.0 10.0
    endloop
  endfacet
  facet normal 0.0 0.0 1.0
    outer loop
      vertex 10.0 -10.0 5.0
      vertex 20.0 -10.0 5.0
      vertex 20.0 0.0 10.0
    endloop
  endfacet

  // Mount point 1 sides
  facet normal 0.0 -1.0 0.0
    outer loop
      vertex 10.0 -10.0 5.0
      vertex 10.0 -10.0 10.0
      vertex 20.0 -10.0 10.0
    endloop
  endfacet
  facet normal 0.0 -1.0 0.0
    outer loop
      vertex 10.0 -10.0 5.0
      vertex 20.0 -10.0 10.0
      vertex 20.0 -10.0 5.0
    endloop
  endfacet

  facet normal -1.0 0.0 0.0
    outer loop
      vertex 10.0 -10.0 5.0
      vertex 10.0 0.0 10.0
      vertex 10.0 -10.0 10.0
