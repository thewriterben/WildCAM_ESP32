    outer loop
      vertex 120.0 0.0 0.0
      vertex 120.0 0.0 5.0
      vertex 120.0 80.0 5.0
    endloop
  endfacet

  // Tilt adjustment mechanism - hinge mount at back
  // Hinge post 1 (left side)
  facet normal 0.0 0.0 1.0
    outer loop
      vertex 20.0 75.0 5.0
      vertex 30.0 85.0 15.0
      vertex 20.0 85.0 15.0
    endloop
  endfacet
  facet normal 0.0 0.0 1.0
    outer loop
      vertex 20.0 75.0 5.0
      vertex 30.0 75.0 5.0
      vertex 30.0 85.0 15.0
    endloop
  endfacet

  // Hinge post 1 sides
  facet normal 0.0 1.0 0.0
    outer loop
      vertex 20.0 85.0 5.0
      vertex 30.0 85.0 15.0
      vertex 20.0 85.0 15.0
    endloop
  endfacet
  facet normal 0.0 1.0 0.0
    outer loop
      vertex 20.0 85.0 5.0
      vertex 30.0 85.0 5.0
      vertex 30.0 85.0 15.0
    endloop
