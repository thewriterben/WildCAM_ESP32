  facet normal 1.0 0.0 0.0
    outer loop
      vertex 20.0 -10.0 5.0
      vertex 20.0 0.0 10.0
      vertex 20.0 0.0 5.0
    endloop
  endfacet
  facet normal 1.0 0.0 0.0
    outer loop
      vertex 20.0 -10.0 5.0
      vertex 20.0 -10.0 10.0
      vertex 20.0 0.0 10.0
    endloop
  endfacet

  // Mount point 2 (right)
  facet normal 0.0 0.0 1.0
    outer loop
      vertex 100.0 -10.0 5.0
      vertex 110.0 0.0 10.0
      vertex 100.0 0.0 10.0
    endloop
  endfacet
  facet normal 0.0 0.0 1.0
    outer loop
      vertex 100.0 -10.0 5.0
      vertex 110.0 -10.0 5.0
      vertex 110.0 0.0 10.0
