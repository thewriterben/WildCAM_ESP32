  endfacet

  facet normal 1.0 0.0 0.0
    outer loop
      vertex 30.0 75.0 5.0
      vertex 30.0 85.0 15.0
      vertex 30.0 85.0 5.0
    endloop
  endfacet
  facet normal 1.0 0.0 0.0
    outer loop
      vertex 30.0 75.0 5.0
      vertex 30.0 75.0 15.0
      vertex 30.0 85.0 15.0
    endloop
  endfacet

  // Hinge post 2 (right side)
  facet normal 0.0 0.0 1.0
    outer loop
      vertex 90.0 75.0 5.0
      vertex 100.0 85.0 15.0
      vertex 90.0 85.0 15.0
    endloop
  endfacet
  facet normal 0.0 0.0 1.0
    outer loop
      vertex 90.0 75.0 5.0
      vertex 100.0 75.0 5.0
      vertex 100.0 85.0 15.0
    endloop
  endfacet

  // Hinge post 2 sides
  facet normal 0.0 1.0 0.0
    outer loop
      vertex 90.0 85.0 5.0
      vertex 100.0 85.0 15.0
      vertex 90.0 85.0 15.0
    endloop
  endfacet
  facet normal 0.0 1.0 0.0
    outer loop
      vertex 90.0 85.0 5.0
      vertex 100.0 85.0 5.0
      vertex 100.0 85.0 15.0
    endloop
  endfacet

  facet normal -1.0 0.0 0.0
    outer loop
      vertex 90.0 75.0 5.0
      vertex 90.0 85.0 15.0
      vertex 90.0 75.0 15.0
    endloop
  endfacet
  facet normal -1.0 0.0 0.0
    outer loop
      vertex 90.0 75.0 5.0
      vertex 90.0 85.0 5.0
      vertex 90.0 85.0 15.0
    endloop
